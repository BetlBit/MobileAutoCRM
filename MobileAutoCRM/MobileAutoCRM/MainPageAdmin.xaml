<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileAutoCRM.MainPageAdmin">
    <ContentPage.Content>
        <StackLayout Margin="15">
            <Button x:Name="buttonToListService" Text="Список сервисов" Clicked="RegPageToPageAdminListService" />
            <Button x:Name="buttonToAddService" Text="Добавить сервис" Clicked="RegPageToPageAdminAddService" />
            <Button x:Name="buttonToDelService" Text="Удалить сервис" Clicked="RegPageToPageAdminDelService" />
            <Button x:Name="buttonToMainPageCust" Text="На главный экран" Clicked="MainPageAdminToMainPageCust" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>